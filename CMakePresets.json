{
	"version": 9,
	"cmakeMinimumRequired": {
		"major": 3,
		"minor": 30
	},
	"configurePresets": [
		{
			"name": "default",
			"hidden": true,
			"architecture": {
				"value": "x64",
				"strategy": "external"
			},
			"binaryDir": "${sourceDir}/Build/${presetName}",
			"installDir": "${sourceDir}/Install/${presetName}",
			"cacheVariables": {
				"RWT_TARGET_CATEGORY": "None"
			}
		},
		{
			"name": "tool",
			"inherits": "default",
			"displayName": "Tool",
			"cacheVariables": {
				"RWT_TARGET_CATEGORY": "Tool"
			}
		}
	],
	"buildPresets": [
		{
			"name": "debug-tool",
			"displayName": "Tool x64 Debug",
			"configurePreset": "tool",
			"targets": [ "Editor" ],
			"configuration": "Debug"
		},
		{
			"name": "release-tool",
			"displayName": "Tool x64 Release",
			"configurePreset": "tool",
			"targets": [ "Editor" ],
			"configuration": "Release"
		}
	]
}
